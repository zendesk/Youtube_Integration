  "repository": "jshttp/mime-db",
  "devDependencies": {
    "bluebird": "3.5.0",
    "co": "4.6.0",
    "cogent": "1.0.1",
    "csv-parse": "1.2.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "gnode": "0.1.2",
    "istanbul": "0.4.5",
    "mocha": "1.21.5",
    "raw-body": "2.2.0",
    "stream-to-array": "2.3.0"
